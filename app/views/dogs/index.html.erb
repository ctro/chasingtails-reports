<h1><%= link_to @client.name, @client %>'s dogs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @client.dogs.each do |dog| %>
      <tr>
        <td><%= link_to dog.name, client_dog_path(@client, dog) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dog', new_client_dog_path(@client) %> | 
<%= link_to 'Back', client_path(@client) %>
